<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(synchronization) -%>">
  <span class='title'>
    <%= synchronization.model_name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.synchronizations_synchronization_path(synchronization),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_synchronizations_admin_synchronization_path(synchronization),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.synchronizations_admin_synchronization_path(synchronization),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => synchronization.model_name),
        :method => :delete %>
  </span>
</li>
